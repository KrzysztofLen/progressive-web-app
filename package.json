{
	"name": "pwa",
	"version": "1.0.0",
	"description": "A complete PWA app",
	"main": "index.js",
	"scripts": {
		"prestart": "babel-node buildScripts/startMessage.js",
		"start": "npm-run-all --parallel security-check dev-server lint:html build",
		"build": "webpack",
		"dev-server": "webpack-dev-server -d --hot --config webpack.config.js",
		"lint:eslint": "esw webpack.config.* public/**/**/*.js buildScripts",
		"lint:html": "node ./buildScripts/html-validator.js",
		"security-check": "nsp check",
		"watch": "webpack --watch",
		"uglifyjs": "uglifyjs public/src/js/idb.js -o public/src/js/idb.min.js --compress --mangle && uglifyjs public/src/js/fetch.js -o public/src/js/fetch.min.js --compress --mangle && uglifyjs public/src/js/promise.js -o public/src/js/promise.min.js --compress --mangle && uglifyjs public/src/js/utility.js -o public/src/js/utility.min.js --compress --mangle && uglifyjs public/src/js/app.js -o public/src/js/app.min.js --compress --mangle && uglifyjs public/src/js/main.js -o public/src/js/main.min.js --compress --mangle",
		"local": "lt --port 3002",
		"selenium:start": "selenium-standalone install && selenium-standalone start",
		"test:e2e": "nightwatch",
		"e2e": "nightwatch test/e2e/nightwatch.conf.js",
		"unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
		"open:coverage": "start chrome ./test/unit/coverage/lcov-report/index.html"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KrzysztofLen/progressive-web-app.git"
	},
	"author": "Krzysztof Len",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/KrzysztofLen/progressive-web-app/issues"
	},
	"devDependencies": {
		"autoprefixer": "^7.1.2",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-minify-webpack-plugin": "^0.2.0",
		"babel-plugin-istanbul": "^3.0.0",
		"babel-plugin-transform-es2015-modules-umd": "^6.24.1",
		"babel-polyfill": "^6.16.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-register": "^6.18.0",
		"chai": "^3.5.0",
		"chalk": "^2.1.0",
		"chromedriver": "^2.33.0",
		"cross-env": "^5.0.1",
		"css-hot-loader": "^1.3.0",
		"css-loader": "^0.28.5",
		"eslint": "^4.5.0",
		"eslint-plugin-import": "^2.7.0",
		"eslint-watch": "^3.1.2",
		"extract-text-webpack-plugin": "^3.0.0",
		"file-loader": "^0.11.2",
		"geckodriver": "^1.9.0",
		"html5-lint": "^0.3.0",
		"http-server": "^0.10.0",
		"image-webpack-loader": "^3.3.1",
		"karma": "^1.4.1",
		"karma-babel-preprocessor": "^7.0.0",
		"karma-chrome-launcher": "^2.2.0",
		"karma-coverage": "^1.1.1",
		"karma-firefox-launcher": "^1.0.1",
		"karma-fixture": "^0.2.6",
		"karma-html2js-preprocessor": "^1.1.0",
		"karma-json-fixtures-preprocessor": "0.0.6",
		"karma-mocha": "^1.3.0",
		"karma-mocha-reporter": "^2.2.4",
		"karma-phantomjs-launcher": "^1.0.2",
		"karma-phantomjs-shim": "^1.4.0",
		"karma-sinon-chai": "^1.3.1",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-spec-reporter": "0.0.31",
		"karma-rollup-preprocessor": "^4.0.0",
		"karma-webpack": "^2.0.2",
		"localtunnel": "^1.8.3",
		"mocha": "^3.2.0",
		"nightwatch": "^0.9.16",
		"npm-run-all": "^4.0.2",
		"nsp": "2.6.2",
		"optimize-css-assets-webpack-plugin": "^3.1.1",
		"optimize-js-plugin": "0.0.4",
		"phantomjs-prebuilt": "^2.1.14",
		"postcss-loader": "^2.0.6",
		"rollup": "^0.47.0",
		"rollup-plugin-babel": "^3.0.0",
		"rollup-plugin-babili": "^3.0.0",
		"rollup-plugin-commonjs": "^8.0.0",
		"rollup-plugin-istanbul": "^1.1.0",
		"rollup-plugin-mockr": "^1.0.1",
		"rollup-plugin-node-resolve": "^3.0.0",
		"rollup-plugin-uglify": "^2.0.0",
		"selenium-server": "^3.0.1",
		"selenium-standalone": "^6.9.0",
		"sinon": "^2.1.0",
		"sinon-chai": "^2.8.0",
		"style-loader": "^0.18.2",
		"uglify-js": "^3.1.2",
		"uglifyjs-webpack-plugin": "^0.4.6",
		"webpack": "^3.5.5",
		"webpack-dev-server": "^2.7.1"
	},
	"dependencies": {
		"material-design-lite": "^1.3.0",
		"uglify-es": "github:mishoo/UglifyJS2#harmony"
	}
}
